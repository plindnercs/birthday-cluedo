mdp

const n=2; // number of players
const bool makeTotal = true; // adds self loops to the winning states if set to true

// player ids
const int id0=0;
const int id1=1;

global turn: [-1..n]; // turn = n indicates that the game has been won, init = -1 since it gets increased after initialization

global winner: [-1..n-1];

global initStep: [0..12] init 0;

global initPlayer0: bool init false;
global initPlayer1: bool init false;

// control variables for the steps of a single round
global dice: bool init false;
global guess: bool init false;
global response: bool init false;

global endTurn: bool init false;
global checkWinner: bool init false;

// variables that state that a player is ready for the update (has shown his card if possible)
global update0: bool init false;
global update1: bool init false;

// variables which represent the cards each player shows the one whose turn it is in response to the guess
global show0: [-1..7];
global show1: [-1..7];

// variables which state if the player whose turn it is has updated his local knowledge based on the cards it got shown
global updateShow0: bool init false;
global updateShow1: bool init false;

// player positions should be global
// we need this to move other players inside the player module when making a suspicion and to set the initial position
global pos0: [0..12] init 9;
global pos1: [0..12] init 10;

// current suspicion (player id and room id)
global charSusp: [4..7];
global roomSusp: [0..3];

// card variables, value means who is currently in possession of the card, n means card is not given to a player, -1 is the initial value before the card is dealt
// global knowledge
global cKitchen: [-1..n];		// room 0 (pos = 0)      (card id 0)
global cLivingRoom: [-1..n];	// room 1 (pos = 2)
global cLibrary: [-1..n];		// room 2 (pos = 6)
global cDiningRoom: [-1..n];	// room 3 (pos = 8)      (card id 3)
global cChar0: [-1..n];		// player 0     (card id 4)
global cChar1: [-1..n];		// player 1
global cChar2: [-1..n];		// player 2
global cChar3: [-1..n];		// player 3     (card id 7)

// global table of NOT knowledge
// 0 -> we don't know if the card is not in possession of player i
// 1 -> we know that the card is not in possession of player i
global ncKitchenPlayer0 : bool init false;
global ncKitchenPlayer1 : bool init false;

global ncLivingRoomPlayer0 : bool init false;
global ncLivingRoomPlayer1 : bool init false;

global ncLibraryPlayer0 : bool init false;
global ncLibraryPlayer1 : bool init false;

global ncDiningRoomPlayer0 : bool init false;
global ncDiningRoomPlayer1 : bool init false;

global ncChar0Player0 : bool init false;
global ncChar0Player1 : bool init false;

global ncChar1Player0 : bool init false;
global ncChar1Player1 : bool init false;

global ncChar2Player0 : bool init false;
global ncChar2Player1 : bool init false;

global ncChar3Player0 : bool init false;
global ncChar3Player1 : bool init false;

formula allInRange = (cntP0 < 3) & (cntP1 < 3);
formula p0InRange = (cntP0 < 3);
formula p1InRange = (cntP1 < 3);

formula updatedShownCards = (updateShow0 = true & updateShow1 = true);

formula cKitchenDecided0 = (cKitchen0 != -1);
formula cLivingRoomDecided0 = (cLivingRoom0 != -1);
formula cLibraryDecided0 = (cLibrary0 != -1);
formula cDiningRoomDecided0 = (cDiningRoom0 != -1);

formula cKitchenDecided1 = (cKitchen0 != -1 | ncKitchenPlayer0 = true);
formula cLivingRoomDecided1 = (cLivingRoom0 != -1 | ncLivingRoomPlayer0 = true);
formula cLibraryDecided1 = (cLibrary0 != -1 | ncLibraryPlayer0 = true);
formula cDiningRoomDecided1 = (cDiningRoom0 != -1 | ncDiningRoomPlayer0 = true);

formula cChar0Decided0 = (cChar00 != -1);
formula cChar1Decided0 = (cChar10 != -1);
formula cChar2Decided0 = (cChar20 != -1);
formula cChar3Decided0 = (cChar30 != -1);

formula cChar0Decided1 = (ncChar0Player0 = true | cChar00 != -1);
formula cChar1Decided1 = (ncChar1Player0 = true | cChar10 != -1);
formula cChar2Decided1 = (ncChar2Player0 = true | cChar20 != -1);
formula cChar3Decided1 = (ncChar3Player0 = true | cChar30 != -1);

formula gameInitialized = (initStep = 12);

module master

	// counter which indicates how many cards a player already has
	cntP0: [0..3];
	cntP1: [0..3];

	// start of initialisation
	[] initStep = 0 -> 0.25: (cKitchen'=n) & (initStep'=1) + 0.25: (cLivingRoom'=n) & (initStep'=1) + 0.25: (cLibrary'=n) & (initStep'=1) + 0.25: (cDiningRoom'=n) & (initStep'=1);
    [] initStep = 1 -> 0.25: (cChar0'=n) & (initStep'=2) + 0.25: (cChar1'=n) & (initStep'=2) + 0.25: (cChar2'=n) & (initStep'=2) + 0.25: (cChar3'=n) & (initStep'=2);

	// here there are 6 cards left to distribute
	[] initStep = 2 & cKitchen != n & cKitchen = -1 -> 1/2: (cKitchen'=0) & (cntP0'=1) & (initStep'=3) + 1/2: (cKitchen'=1) & (cntP1'=1) & (initStep'=3);
	[] initStep = 2 & cKitchen = n -> (initStep' = 3) & (cKitchen'=n);
	[] initStep = 3 & allInRange & cLivingRoom != n & cLivingRoom = -1 -> 1/2: (cLivingRoom'=0) & (initStep' = 4) & (cntP0'=cntP0+1) + 1/2: (cLivingRoom'=1) & (initStep' = 4) & (cntP1'=cntP1+1);
	[] initStep = 3 & cLivingRoom = n -> (initStep' = 4);
	[] initStep = 4 & allInRange & cLibrary != n & cLibrary = -1 -> 1/2: (cLibrary'=0) & (initStep' = 5) & (cntP0'=cntP0+1) + 1/2: (cLibrary'=1) & (initStep' = 5) & (cntP1'=cntP1+1);
    [] initStep = 4 & cLibrary = n -> (initStep' = 5);

	// assigning dining room card to a player
	// we already distributed 3 cards now
	[] initStep = 5 & cDiningRoom = n -> (initStep'=6);
	[] initStep = 5 & cDiningRoom != n & cDiningRoom = -1 & cntP0 != 3 & cntP1 != 3 -> 1/2: (cDiningRoom'=0) & (initStep'=6) & (cntP0'=cntP0 + 1) + 1/2: (cDiningRoom'=1) & (initStep'=6) & (cntP1'=cntP1 + 1);
	[] initStep = 5 & cDiningRoom != n & cDiningRoom = -1 & cntP0 = 3 & p1InRange -> (cDiningRoom'=1) & (initStep'=6) & (cntP1'=cntP1 + 1);
	[] initStep = 5 & cDiningRoom != n & cDiningRoom = -1 & cntP1 = 3 & p0InRange -> (cDiningRoom'=0) & (initStep'=6) & (cntP0'=cntP0 + 1);

    // we already distributed 4 cards now
	[] initStep = 6 & cChar0 = n -> (initStep'=7);
	[] initStep = 6 & cChar0 != n & cChar0 = -1 & cntP0 != 3 & cntP1 != 3 -> 1/2: (cChar0'=0) & (initStep'=7) & (cntP0'=cntP0 + 1) + 1/2: (cChar0'=1) & (initStep'=7) & (cntP1'=cntP1 + 1);
    [] initStep = 6 & cChar0 != n & cChar0 = -1 & cntP0 = 3 & p1InRange -> (cChar0'=1) & (initStep'=7) & (cntP1'=cntP1 + 1);
    [] initStep = 6 & cChar0 != n & cChar0 = -1 & cntP1 = 3 & p0InRange -> (cChar0'=0) & (initStep'=7) & (cntP0'=cntP0 + 1);

    // we already distributed 5 cards now
	[] initStep = 7 & cChar1 = n -> (initStep'=8);
	[] initStep = 7 & cChar1 != n & cChar1 = -1 & cntP0 != 3 & cntP1 != 3 -> 1/2: (cChar1'=0) & (initStep'=8) & (cntP0'=cntP0 + 1) + 1/2: (cChar1'=1) & (initStep'=8) & (cntP1'=cntP1 + 1);
    [] initStep = 7 & cChar1 != n & cChar1 = -1 & cntP0 = 3 & p1InRange -> (cChar1'=1) & (initStep'=8) & (cntP1'=cntP1 + 1);
    [] initStep = 7 & cChar1 != n & cChar1 = -1 & cntP1 = 3 & p0InRange -> (cChar1'=0) & (initStep'=8) & (cntP0'=cntP0 + 1);

    // we already distributed 6 cards now
	[] initStep = 8 & cChar2 = n -> (initStep'=9);
    [] initStep = 8 & cChar2 != n & cChar2 = -1 & cntP0 != 3 & cntP1 != 3 -> 1/2: (cChar2'=0) & (initStep'=9) & (cntP0'=cntP0 + 1) + 1/2: (cChar2'=1) & (initStep'=9) & (cntP1'=cntP1 + 1);
	[] initStep = 8 & p1InRange & cChar2 != n & cChar2 = -1 & cntP0 = 3 & cntP1 < 3 -> (cChar2'=1) & (initStep'=9) & (cntP1' = cntP1 + 1);
	[] initStep = 8 & p0InRange & cChar2 != n & cChar2 = -1 & cntP1 = 3 & cntP0 < 3 -> (cChar2'=0) & (initStep'=9) & (cntP0' = cntP0 + 1);

	// maybe we still need to distribute cChar3
	[] initStep = 9 & cChar3 = n -> (initStep'=10);
	[] initStep = 9 & cChar3 != n & cChar3 = -1 & cntP0 < 3 & cntP1 = 3 -> (cChar3'=0) & (initStep'=10) & (cntP0' = cntP0 + 1);
	[] initStep = 9 & cChar3 != n & cChar3 = -1 & cntP1 < 3 & cntP0 = 3 -> (cChar3'=1) & (initStep'=10) & (cntP1' = cntP1 + 1);

	// the last step is to set the players to their initial position
	[] initStep = 10 -> (pos0'=9) & (pos1'=10) & (initStep'=11);

	// initialization done once all players saved their cards locally
	[] initStep = 11 & initPlayer0 = true & initPlayer1 = true -> (initStep' = 12) & (endTurn' = true);

	// here we end the current round and prepare it for the next player in a round robin fashion
    [] gameInitialized & endTurn = true & checkWinner = false -> (turn' = mod(turn + 1, n)) & (dice' = true) & (endTurn' = false) & (update0' = false) & (update1' = false) & (show0' = -1) & (show1' = -1) & (updateShow0' = false) & (updateShow1' = false) & (guess' = false) & (response' = false);

endmodule

